      }\n    }\n  }\n\n  return w;\n}\n\nfunction containPath(data, lineWidth, isStroke, x, y) {\n  var w = 0;\n  var xi = 0;\n  var yi = 0;\n  var x0 = 0;\n  var y0 = 0;\n\n  for (var i = 0; i < data.length;) {\n    var cmd = data[i++]; // Begin a new subpath\n\n    if (cmd === CMD.M && i > 1) {\n      // Close previous subpath\n      if (!isStroke) {\n        w += windingLine(xi, yi, x0, y0, x, y);\n      } // 如果被任何一个 subpath 包含\n      // if (w !== 0) {\n      //     return true;\n      // }\n\n    }\n\n    if (i == 1) {\n      // 如果第一个命令是 L, C, Q\n      // 则 previous point 同绘制命令的第一个 point\n      //\n      // 第一个命令为 Arc 的情况下会在后面特殊处理\n      xi = data[i];\n      yi = data[i + 1];\n      x0 = xi;\n      y0 = yi;\n    }\n\n    switch (cmd) {{"type":"NormalModule","moduleId":1073,"context":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/lib/model/mixin","request":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/lib/model/mixin/itemStyle.js","userRequest":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/lib/model/mixin/itemStyle.js","rawRequest":"./mixin/itemStyle","resource":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/lib/model/mixin/itemStyle.js","loaders":[],"identifier":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/lib/model/mixin/itemStyle.js","buildTimestamp":1520924854829,"strict":false,"meta":{},"used":true,"usedExports":true,"providedExports":true,"issuer":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/lib/model/Model.js","rawSource":"var makeStyleMapper = require(\"./makeStyleMapper\");\n\nvar getItemStyle = makeStyleMapper([['fill', 'color'], ['stroke', 'borderColor'], ['lineWidth', 'borderWidth'], ['opacity'], ['shadowBlur'], ['shadowOffsetX'], ['shadowOffsetY'], ['shadowColor'], ['textPosition'], ['textAlign']]);\nvar _default = {\n  getItemStyle: function (excludes, includes) {\n    var style = getItemStyle(this, excludes