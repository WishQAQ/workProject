},\n            modalRightData: {\n                patientLog: {},\n                shiftChangeMaster: {},\n                statistics: {}\n            },\n            time: ''\n        };\n        /**\n         * 设置值: 公共对外值改变\n         */\n        this.setStateJson2 = (path, data) => {\n            this.dispatch2(JsonUtil.json(path, this.state, data));\n        };\n        /**\n         * 弹框关闭事件\n         */\n        this.modalClose = () => {\n            this.dispatch({\n                modalVisible: false\n            });\n        };\n        /**\n         * 弹框打开事件\n         * @param {object} data 打开弹框所要展示的数据\n         */\n        this.openModal = (data) => {\n            let param = {\n                wardCode: data.wardCode,\n                classesId: data.classesId,\n                patientId: data.patientId,\n                visitId: data.visitId,\n                radio: data.radio === 2 ? null : data.radio,\n                time: moment(data.time).format('YYYY-MM-DD') // 时间\n            };\n            ApiShiftChange.finShiftByRecord(param.wardCode, param.classesId, param.patientId, param.visitId, param.radio, param.time).then((data) => {\n                if (data && data.length) {\n                    this.dispatch2({\n                        modalLeftData: data,\n                        modalVisible: true,\n                        param: param,\n                        radioValue: param.radio,\n                        time: param.time\n                    });\n                }\n                else {\n                    this.dispatch2({\n                        modalLeftData: data,\n                        param: param,\n                        radioValue: param.radio,\n                        time: param.time,\n                        modalRightData: {\n                            patientLog: {},\n                            shiftChangeMaster: {},\n                            statistics: {}\n                        },\n                    });\n                    message.tip('暂无交接记录', 'info', 'center');\n                }\n            }).catch(err => {\n                message.tip(err || '获取信息失败', 'error', 'center');\n            });\n        };\n        /**\n         * 点击全部、已交接、未交接\n         * @param e\n         */\n        this.changeModalLeftData = (e) => {\n            let { param } = this.state;\n            param.radio = Number.parseInt(e.value);\n            this.openModal(param);\n        };\n        /**\n         * 更改事件\n         * @param v\n         */\n        this.changeTime = (v) => {\n            let { param } = this.state;\n            param.time = moment(v).format('YYYY-MM-DD');\n            this.openModal(param);\n            this.dispatch({\n                time: moment(v).format('YYYY-MM-DD')\n            });\n        };\n        /**\n         * 左侧表格点击事件\n         * @param rowData  点击表格行数据\n         */\n        this.selectDetail = (rowData) => {\n            const { param } = this.state;\n            ApiShiftChange.finShiftByRecordInfo(rowData.id, param.wardCode, rowData.patientLog).then((data) => {\n                this.dispatch({ modalRightData: data });\n            }).catch(err => {\n                message.tip(err || '获取信息失败', 'error', 'center');\n            });\n        };\n        /**\n         * 渲染表格\n         * @param params {}\n         */\n        this.onGridReady = (params) => {\n            this.tableApi = params.api;\n        };\n        /**\n         * 根据是否交接设置每行显示颜色\n         * @param params 数据\n         * @param style 样式\n         * @returns {any}\n         */\n        this.setRowClass = (params, style) => {\n            let style1;\n            style1 = `${style.agCommonCell}`;\n            if (params.data.status === '未交班')\n                style1 = `${style.agRedCell}`;\n            return style1;\n        };\n        /**\n         * 交接按钮事件\n         */\n        this.shiftBtn = (e) => {\n            let { patientLog } = this.state.modalRightData;\n            if (!patientLog.id) {\n                patientLog.id = loginService.state.loginSession.user.empNo;\n            }\n            if (e === '交班') {\n                ApiShiftChange.savaLastUpdateUser(patientLog.id).then((data) => {\n                    message.tip(data || '交班成功', 'success', 'center');\n                    this.dispatch2({\n                        modalVisible: false,\n                        modalRightData: {\n                            patientLog: {},\n                            shiftChangeMaster: {},\n                            statistics: {}\n                        },\n                        edit: false\n                    });\n                }).catch(err => {\n                    message.tip(err || '交班失败', 'error', 'center');\n                });\n            }\n            if (e === '接班') {\n                ApiShiftChange.takeShift(patientLog.id).then((data) => {\n                    message.tip(data || '接班成功', 'success', 'center');\n                    this.dispatch2({ edit1: false });\n                }).catch(err => {\n                    message.tip(err || '接班失败', 'error', 'center');\n                });\n            }\n        };\n        this.addPatientLog = () => {\n            let { modalRightData } = this.state;\n            ApiShiftChange.updatePatientLog(modalRightData.patientLog).then((data) => {\n                message.tip(data || '保存成功', 'success', 'center');\n            }).catch(err => {\n                message.tip(err || '保存失败', 'error', 'center');\n            });\n        };\n        /**\n         * input框赋值的方法\n         */\n        this.inputOnchage = (name, e) => {\n            let anys = name.split('.');\n            let { modalRightData } = this.state;\n            modalRightData.patientLog[anys[1]] = e.target.value;\n            this.dispatch({ modalRightData });\n        };\n        /**\n         * 多行文本框值改变的方法\n         */\n        this.textAreaOnchange = (name, e) => {\n            let anys = name.split('.');\n            let { modalRightData } = this.state;\n            modalRightData.patientLog[anys[1]] = e.target.value;\n            this.dispatch({ modalRightData });\n        };\n    }\n    /**\n     * 页面加载执行\n     */\n    serviceWillMount() {\n        // console\n    }\n}\nexport const shiftRecordService = new ShiftRecordService('shiftRecord');\n","source":"/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_tools_flux_BaseService__ = __webpack_require__(52);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_1_tools_api_JsonUtil__ = __webpack_require__(58);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_2_pkg_common_message__ = __webpack_require__(56);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_3_pkg_api_nurse__ = __webpack_require__(143);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_3_pkg_api_nurse___default = __webpack_require__.n(__WEBPACK_IMPORTED_MODULE_3_pkg_api_nurse__);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_4_moment__ = __webpack_require__(74);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_4_moment___default = __webpack_require__.n(__WEBPACK_IMPORTED_MODULE_4_moment__);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_5_service_user_login__ = __webpack_require__(86);\n\n\n\n\n\n\nclass ShiftRecordService extends __WEBPACK_IMPORTED_MODULE_0_tools_flux_BaseService__[\"a\" /* BaseService */] {\n    constructor() {\n        super(...arguments);\n        this.defaultState = {\n            modalVisible: false,\n            modalLeftData: [],\n            param: {},\n            modalRightData: {\n                patientLog: {},\n                shiftChangeMaster: {},\n                statistics: {}\n            },\n            time: ''\n        };\n        /**\n         * 设置值: 公共对外值改变\n         */\n        this.setStateJson2 = (path, data) => {\n            this.dispatch2(__WEBPACK_IMPORTED_MODULE_1_tools_api_JsonUtil__[\"a\" /* JsonUtil */].json(path, this.state, data));\n        };\n        /**\n         * 弹框关闭事件\n         */\n        