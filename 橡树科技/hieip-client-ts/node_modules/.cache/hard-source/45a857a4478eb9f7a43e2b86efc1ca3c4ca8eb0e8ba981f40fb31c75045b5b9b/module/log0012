urse'\nimport {NurseShiftChangeMasterModelDtoShift} from 'pkg/entity/nurse'\nimport moment from 'moment'\nimport {loginService} from 'service/user/login'\n\nexport interface ParamState {\n    wardCode?: string,// \n    classesId?: number,// \n    patientId?: string,// id\n    visitId?: number, // \n    radio?: number, // \n    time?: string // \n}\nexport interface ShiftRecordState {\n    modalVisible?: boolean,// \n    modalLeftData?: Array<NurseShiftChangeMasterModelDtoShift>,// \n    modalRightData?: any,\n    radioValue?: number, // ,\n    time?: string\n    param?: ParamState\n    edit?:boolean\n    edit1?:boolean\n}\n\nclass ShiftRecordService extends BaseService<ShiftRecordState> {\n    /**\n     * table api\n     * @type {null}\n     */\n    tableApi?: any\n    defaultState = {\n        modalVisible: false,\n        modalLeftData: [],\n        param: {},\n        modalRightData: {\n            patientLog: {},\n            shiftChangeMaster: {},\n            statistics: {}\n        },\n        time: ''\n    }\n\n    /**\n     * \n     */\n    serviceWillMount() {\n        // console\n    }{"type":"NormalModule","moduleId":502,"context":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/dist","request":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/dist/echarts.js","userRequest":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/dist/echarts.js","rawRequest":"echarts/dist/echarts","resource":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/dist/echarts.js","loaders":[],"identifier":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/echarts/dist/echarts.js","buildTimestamp":1520924843105,"strict":false,"meta":{},"used":true,"usedExports":["default"],"providedExports":true,"issuer":"/Users/wujunlong/Documents/work/hieip-client-ts/node_modules/awesome-typescript-loader/dist/entry.js!/Users/wujunlong/Documents/work/hieip-client-ts/src/packages/common/quality-control-echart