?: any\n    defaultState = {\n        modalVisible: false,\n        modalLeftData: [],\n        param: {},\n        modalRightData: {\n            patientLog: {},\n            shiftChangeMaster: {},\n            statistics: {}\n        },\n        time: ''\n    }\n\n    /**\n     * \n     */\n    serviceWillMount() {\n        // console\n    }\n\n    /**\n     * : \n     */\n    setStateJson2 = (path, data) => {\n        this.dispatch2(JsonUtil.json(path, this.state, data))\n    }\n    /**\n     * \n     */\n    modalClose = () => {\n        this.dispatch({\n            modalVisible: false\n        })\n    }\n    /**\n     * \n     * @param {object} data \n     */\n    openModal = (data?: ParamState) => {\n        let param = {\n            wardCode: data.wardCode,// \n            classesId: data.classesId,// \n            patientId: data.patientId,// id\n            visitId: data.visitId, // \n            radio: data.radio === 2 ? null : data.radio, // \n            time: moment(data.time).format('YYYY-MM-DD') // \n        }\n        ApiShiftChange.finShiftByRecord(param.wardCode, param.classesId,\n            param.patientId, param.visitId, param.radio, param.time).then((data: any) => {\n            if (data && data.length) {\n                this.dispatch2({\n                    modalLeftData: data,\n                    modalVisible: true,\n                    param: param,\n                    radioValue: param.radio,\n                    time: param.time\n                })\n            } else {\n                this.dispatch2({\n                    modalLeftData: data,\n                    param: param,\n                    radioValue: param.radio,\n                    time: param.time,\n                    modalRightData: {\n                        patientLog: {},\n                        shiftChangeMaster: {},\n                        statistics: {}\n                    },\n                })\n                message.tip('', 'info', 'center')\n            }\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n    /**\n     * \n     * @param e\n     */\n    changeModalLeftData = (e) => {\n        let {param} = this.state\n        param.radio = Number.parseInt(e.value)\n        this.openModal(param)\n    }\n    /**\n     * \n     * @param v\n     */\n    changeTime = (v) => {\n        let {param} = this.state\n        param.time = moment(v).format('YYYY-MM-DD')\n        this.openModal(param)\n        this.dispatch({\n            time: moment(v).format('YYYY-MM-DD')\n        })\n    }\n    /**\n     * \n     * @param rowData  \n     */\n    selectDetail = (rowData) => {\n        const {param} = this.state\n        ApiShiftChange.finShiftByRecordInfo(rowData.id, param.wardCode, rowData.patientLog).then((data) => {\n            this.dispatch({modalRightData: data})\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n    /**\n     * \n     * @param params {}\n     */\n    onGridReady = (params) => {\n        this.tableApi = params.api\n    }\n    /**\n     * \n     * @param params \n     * @param style \n     * @returns {any}\n     */\n    setRowClass = (params, style) => {\n        let style1\n        style1 = `${style.agCommonCell}`\n        if (params.data.status === '') style1 = `${style.agRedCell}`\n        return style1\n    }\n\n    /**\n     * \n     */\n    shiftBtn = (e) => {\n        let {patientLog} = this.state.modalRightData\n        if (!patientLog.id) {\n            patientLog.id = loginService.state.loginSession.user.empNo\n        }\n        if (e === '') {\n            ApiShiftChange.savaLastUpdateUser(patientLog.id).then((data) => {\n                message.tip(data || '', 'success', 'center')\n                this.dispatch2({\n                    modalVisible: false,\n                    modalRightData: {\n                        patientLog: {},\n                        shiftChangeMaster: {},\n             