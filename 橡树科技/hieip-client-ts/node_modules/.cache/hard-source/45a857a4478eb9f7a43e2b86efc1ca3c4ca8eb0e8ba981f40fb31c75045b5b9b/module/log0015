n)\n        for (let i = 0; i < keys.length; i++) {\n            const key = keys[i]\n            const val = json[key]\n            if (typeof val === 'object' && 'hash' in val) {\n                json[key].show = this.hasAdmin ? true : this.state.userPrint[val.hash] ? this.state.userPrint[val.hash] : false\n            } else if (typeof val === 'object') this.resetPointKey(val)\n        }\n    }\n\n    get hasAdmin() {\n        return parseInt(config.admin, 0) === hash.hashCode(loginService.state.username)\n    }\n\n    refreshMenus(menu: any) {\n        const keys = Object.keys(menu)\n        const menuObject = [] as Menu[]\n        for (let i = 0; i < keys.length; i++) {\n            const key = keys[i]\n            if (key === 'icon') continue\n            const val = menu[key]\n            if ('hash' in val) {\n                if (this.hasAdmin || this.state.userPrint[val.hash] === true) {\n                    menuObject.push({\n                        name: key,\n                        route: val.route\n                    })\n                }\n            } else {\n                menuObject.push({\n                    name: key,\n                    icon: val.icon,\n                    children: this.refreshMenus(val)\n                })\n            }\n        }\n        return menuObject\n    }\n\n    resetPoint(data) {\n        if (!this.hasAdmin && (!data || !data.menuAndPointJson)) {\n            throw Error('')\n        }\n        this.dispatch2({ userPrint: this.hasAdmin ? {} : JSON.parse(data.menuAndPointJson.value) })\n        // ,\n        if (AuthService.defaultPoint === null) {\n            AuthService.defaultPoint = extend(true, {}, point)\n        }\n        if (AuthService.defaultSubMenus === null) {\n            AuthService.defaultSubMenus = extend(true, {}, subMenus)\n        }\n        const copyPoint = extend(true, {}, AuthService.defaultPoint)\n        this.resetPointKey(copyPoint)\n        // 1.\n        extend(true, point, copyPoint)\n        localStorage.setItem('point', JSON.stringify(point))\n        // 2.\n        menuService.setMenus(this.refreshMenus(menus))\n        // 3. \n        const copySubMenus = extend(true, {}, AuthService.defaultSubMenus)\n        this.resetSubMenus(copySubMenus)\n        // extend(true, subMenus, copySubMenus)\n        // 4. \n        routeService.push(this.hasAdmin ? 'route_new_triage' : data.menuAndPointJson.defaultMenu)\n    }\n}\nconst point2 = localStorage.getItem('point')\nif (point2) {\n    extend(true, point, JSON.parse(point2))\n}\nexport const authService = new AuthService('auth')"]}},"assets":[],"hashContent":"meta{\"harmonyModule\":true}504true[\"authService\"]86true52true505true85undefined506true124true574true507true434true575true52BaseService__WEBPACK_IMPORTED_MODULE_1_tools_flux_BaseService__atruetrue[\"BaseService\"]434config__WEBPACK_IMPORTED_MODULE_8_tools_config__atruetrue[\"config\"]575hash__WEBPACK_IMPORTED_MODULE_9_tools_hash__atruetrue[\"hash\"]86loginService__WEBPACK_IMPORTED_MODULE_0_service_user_login_index__atruetrue[\"loginService\"]85extend__WEBPACK_IMPORTED_MODULE_3_jquery__extendundefinedtruetrue505point__WEBPACK_IMPORTED_MODULE_2_pkg_entity_point__atruetrue[\"point\"]85extend__WEBPACK_IMPORTED_MODULE_3_jquery__extendundefinedtruetrue507subMenus__WEBPACK_IMPORTED_MODULE_7_pkg_entity_subMenus__atruetrue[\"subMenus\"]85extend__WEBPACK_IMPORTED_MODULE_3_jquery__extendundefinedtruetrue85extend__WEBPACK_IMPORTED_MODULE_3_jquery__extendundefinedtruetrue505point__WEBPACK_IMPORTED_MODULE_2_pkg_entity_point__atruetrue[\"point\"]505point__WEBPACK_IMPORTED_MODULE_2_pkg_entity_point__atruetrue[\"point\"]506menuService__WEBPACK_IMPORTED_MODULE_4_service_home_menu_index__atruetrue[\"menuService\"]574menus__WEBPACK_IMPORTED_MODULE_6_pkg_entity_menus__atruetrue[\"menus\"]85extend__WEBPACK_IMPORTED_MODULE_3_jquery__extendundefinedtruetrue124routeService__WEBPACK_IMPORTED_MODULE_5_service_RouteService__atruetrue[\"routeService\"]85extend__WEBPACK_I