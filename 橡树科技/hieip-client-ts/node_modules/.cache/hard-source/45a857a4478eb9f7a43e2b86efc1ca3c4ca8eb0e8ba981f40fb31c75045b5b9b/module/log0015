eType?: number\n    /**  */\n    names?: string\n    /**  */\n    page?: Page\n    /**  */\n    monitorCode?: string\n    /**  */\n    deptCode?: string\n    /** id */\n    patientId?: string\n    /**  */\n    visitId?: number\n    /**  */\n    templateIndex?: MrTemplateIndexModelDtoTemplate[]\n    /**  */\n    mrModle?:MrTemplateClassModelDtoTemplate[]\n    /** input  */\n    dictCode?: string\n    /** input   */\n    dictCode1?:string\n    /**  input   */\n    inputPage?:Page\n    /**  input   */\n    inputCode?: string\n    /** input  */\n    inputLength?:number\n    /** input  */\n    inputLength1?:number\n    /**  input   */\n    inputDate?:any\n    /**  input   */\n    inputDate1?:any\n    /** input title */\n    inputTitle?:any[]\n    /** input title  */\n    inputTitle1?:any[]\n    /**  */\n    errorMessage?:string\n    /**  */\n    operation?:string\n    /**   */\n    templateLenght?:number\n    /**   */\n    classify?:string\n    /**   */\n    template?:string\n    /**   */\n    mrTemplate?:MrTemplateIndexModelDtoTemplate\n    /**   */\n    visible?:boolean\n    /*  */\n    time?:any\n    times?:any\n}\nclass NewfileService extends BaseService<NewfileState> {\n    mainagApi:any\n    defaultState = {\n        names:'',\n        monitorcode:'',\n        dictCode:'YSZC',\n        inputPage: {\n            startIndex: 1,\n            pageSize: 6\n        },\n        inputLength:0,\n        dictCode1:'YHXX',\n        inputPage1: {\n            startIndex: 1,\n            pageSize: 6\n        },\n        inputLength1:0,\n        operation:'',\n        templateLenght:0,\n        page:{pageSize:100,startIndex:1},\n        tempaleType:1,\n        visible:false,\n        times:''\n    }\n    /**\n     * \n     */\n    patient=(id?:string ,visit?:number,dept?:string,classs?:string,visibles?:boolean)=>{\n     this.dispatch({patientId:id,visitId:visit,deptCode:dept,mrClass:classs,visible:visibles})\n        this.findMrTemplate()\n    }\n\n    /**\n     * \n     */\n    findByPage = (starIndex?) => {\n        let {mrClass, tempaleType, names, page, deptCode, patientId, visitId,times} = this.state\n        page.startIndex = starIndex ? starIndex : page.startIndex\n        return new Promise(resolve => {\n            return ApiTemplateMrIndex.findByPage(mrClass, tempaleType, names, page, times,\n                '', deptCode, patientId, visitId).then(data => {\n                resolve(data)\n            }).catch(err => {\n                message.tip(err || '', 'error', 'center')\n            })\n        } )\n\n    }\n\n    /**\n     * \n     */\n    findMrTemplate=()=>{\n        return ApiTemplateMrClass.findMrTemplate().then((data: any) => {\n            this.dispatch({mrModle:data})\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n\n    /**\n     * input tltle\n     */\n    loadColumns=(code?:string)=>{\n        return ApiDictInput.loadColumns(code).then((data) => {\n            if (code==='YSZC'){\n                this.dispatch({inputTitle:data})\n            }else{\n                this.dispatch({inputTitle1:data})\n            }\n\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n    /**\n     * input \n     */\n    loadData=(code?:string,page?:Page,inputName?:string)=>{\n        return ApiDictInput.loadData(page,code,inputName).then((data) => {\n            if (code==='YSZC'){\n                this.dispatch2({inputDate:data,inputLength:data.total})\n            }else{\n                this.dispatch2({inputDate1:data,inputLength1:data.total})\n            }\n\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n\n    /**\n     * \n     */\n    showMessage = (v,name?:string) => {\n        let page={}\n        let code=''\n        let inputName=''\n        switch (v.type) {\n            case 'pageEvent': {\n                   page={pageSize: v.pageSize, startIndex: v.pageCurrent}\n                   code=name\n                   inputName=v.value\n                   this.loadData(code,page,inputName)\n                   break\n            }\n            case 'enterEvent': {\n                this.dispatch(JsonUtil.json('', this.state,v.data.id ? v.data.id : ''))\n                break\n            }\n            case 'changeEvent': {\n                page={pageSize: v.pageSize, startIndex: v.pageCurrent}\n                code=name\n                inputName=v.value\n                this.loadData(code,page,inputName)\n                break\n            }\n            default:\n                     page={pageSize: 6, startIndex: 1}\n                     code=name\n                     inputName=v.value\n                     this.loadData(code,page,inputName)\n                     this.loadColumns(code)\n        }\n    }\n\n    /**\n     * \n     */\n    onGridReady = (params) => {\n        this.mainagApi= params\n        this.onReverSource()\n    }\n\n    /**\n     * \n     */\n    onReverSource = () => {\n        let dataSource = {\n            rowCount: null,\n            getRows: (params) => {\n                this.findByPage(params.startRow).then((data: any) => {\n                    params.successCallback(data, data.total?data.total:0)\n                })\n            }\n        }\n        this.mainagApi.api.setDatasource(dataSource)\n    }\n\n    /**\n     * \n     */\n    templateIndexOpt=(params)=>{\n        this.dispatch({mrTemplate:params.data})\n    }\n\n    /**\n     * \n     */\n    inputChange = (e) =>{\n        this.dispatch2({names: e.target.value})\n    }\n\n    /**\n     * \n     */\n    onchange=(name,e)=>{\n        return new Promise(resolve => {\n            let {page}=this.state\n            page.startIndex=1\n            this.dispatch2({[name]:e?e:'',page})\n            resolve()\n        }).then(()=> this.onReverSource())\n    }\n\n    showOnchange=(name,e)=>{\n        this.dispatch2({[name]:e?e:''})\n    }\n\n    /**\n     * \n     */\n    onClick=()=>{\n        return new Promise(resolve => {\n            let {page}=this.state\n            page.startIndex=1\n            this.dispatch2({page})\n            resolve()\n        }).then(()=> this.onReverSource())\n    }\n    /**\n     * \n     */\n    rodisOnchange=(name,e)=>{\n        return new Promise(resolve => {\n            let {page}=this.state\n            page.startIndex=1\n            this.dispatch2({[name]:e.target.value,page})\n            resolve()\n        }).then(()=> this.onReverSource())\n    }\n\n    /*  */\n    onOk=()=>{\n        let{mrTemplate,mrClass}=this.state\n        this.dispatch2({mrTemplate:{modifyTopicTitle:0,needParentSignFlag:0,modifyTopic:0},visible:false,time:'',tempaleType:1})\n        setTimeout(()=>{\n            medicalService.insertPanes(mrTemplate)\n        },100)\n    }\n\n    /**\n     * \n     */\n    cancelEmpty=()=>{\n        this.dispatch2({mrTemplate:{modifyTopicTitle:0,needParentSignFlag:0,modifyTopic:0},visible:false,time:'',tempaleType:1})\n    }\n    /**\n     * \n     */\n    onUpdate=(name?:string,e?:any)=>{\n        this.dispatch2(JsonUtil.json(name,this.state, e.target.value))\n    }\n}\n\nexport const newfileServiceService = new NewfileService('newfile')"]},"baseMapRemovedQuality":"","baseMap":{"version":3,"sources":["/Users/wujunlong/Documents/work/hieip-client-ts/src/service/medical/medical/components/written/new-file/index.ts"],"names":[],"mappings":";;;;;;AAA0E;AAGH;AAE1B;AACgB;AACnB;AA4D1C,oBAAqB,SAAQ,kFAAyB;IAAtD;;QAEI,iBAAY,GAAG;YACX,KAAK,EAAC,EAAE;YACR,WAAW,EAAC,EAAE;YACd,QAAQ,EAAC,MAAM;YACf,SAAS,EAAE;gBACP,UAAU,EAAE,CAAC;gBACb,QAAQ,EAAE,CAAC;aACd;YACD,WAAW,EAAC,CAAC;YACb,SAAS,EAAC,MAAM;YAChB,UAAU,EAAE;gBACR,UAAU,EAAE,CAAC;gBACb,QAAQ,EAAE,CAAC;aACd;YACD,YAAY,EAAC,CAAC;YACd,SAAS,EAAC,EAAE;YACZ,cAAc,EAAC,CAAC;YAChB,IAAI,EAAC,EAAC,QAAQ,EAAC,GAAG,EAAC,UAAU,EAAC,CAAC,EAAC;YAChC,WAAW,EAAC,CAAC;YACb,OAAO,EAAC,KAAK;YACb,KAAK,EAAC,EAAE;SACX;QACD;;WAEG;QACH,YAAO,GAAC,CAAC,EAAU,EAA