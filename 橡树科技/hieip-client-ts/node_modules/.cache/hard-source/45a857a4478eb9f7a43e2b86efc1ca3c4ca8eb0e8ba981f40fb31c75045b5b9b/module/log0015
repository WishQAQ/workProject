(name) {\n        this.storage.removeItem(name);\n    }\n}\nclass StoreEntity {\n    constructor() {\n        this.local = new StoreAtt(window.localStorage);\n        this.session = new StoreAtt(window.sessionStorage);\n        this.dbMap = new Map();\n    }\n    // noinspection JSUnusedGlobalSymbols\n    get defaultDB() {\n        if (!this._defaultDB)\n            this._defaultDB = this.db('default');\n        return this._defaultDB;\n    }\n    db(dbName) {\n        let _dbName = 'default_' + dbName;\n        let db = this.dbMap.get(_dbName);\n        if (!db)\n            this.dbMap.set(_dbName, db = localForage.createInstance({\n                name: _dbName,\n                version: parseInt(window.localStorage.getItem('db_version'), 10) || 1\n            }));\n        return db;\n    }\n}\nexport const store = new StoreEntity();\n","source":"/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_localforage__ = __webpack_require__(568);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_localforage___default = __webpack_require__.n(__WEBPACK_IMPORTED_MODULE_0_localforage__);\n/**\n *\n * Created by jahv on 2017/4/12.\n */\n\n__WEBPACK_IMPORTED_MODULE_0_localforage___default.a.config({\n    driver: __WEBPACK_IMPORTED_MODULE_0_localforage___default.a.INDEXEDDB,\n    name: 'OAK-DB',\n    storeName: 'default',\n    version: 1.0,\n    description: ''\n});\nclass StoreAtt {\n    constructor(storage) {\n        this.storage = storage;\n    }\n    get(name) {\n        let value = this.storage.getItem(name);\n        if (!value)\n            return null;\n        return JSON.parse(value);\n    }\n    // noinspection JSUnusedGlobalSymbols\n    append(name, value) {\n        let old = this.storage.getItem(name);\n        if (old)\n            old = JSON.parse(old);\n        else\n            old = [];\n        if (typeof old.length !== 'number')\n            old = [old];\n        old.push(value);\n        this.storage.setItem(name, JSON.stringify(old));\n    }\n    se