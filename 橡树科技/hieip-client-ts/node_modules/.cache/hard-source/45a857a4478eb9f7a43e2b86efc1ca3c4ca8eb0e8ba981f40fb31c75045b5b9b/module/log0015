e1: data, inputLength1: data.total });\n                }\n            }).catch(err => {\n                message.tip(err || '', 'error', 'center');\n            });\n        };\n        /**\n         * \n         */\n        this.subUpdate = () => {\n            let { mrPatientsModle } = this.state;\n            return ApiPatientMrFileIndex.SubUpdate(mrPatientsModle, 1, '', '').then((data) => {\n                this.dispatch({ visible: false });\n                message.success('');\n            }).catch(err => {\n                message.tip(err || '', 'error', 'center');\n            });\n        };\n        /**\n         * \n         */\n        this.updateInfo = (name) => {\n            if (name === 'YHXX') {\n                JsonUtil.json('mrPatientsModle.topicDoctorName', this.state, this);\n                JsonUtil.json('mrPatientsModle.topicDoctorId', this.state, this);\n            }\n            else {\n                JsonUtil.json('mrPatientsModle.topicTitleName', this.state, this);\n                JsonUtil.json('mrPatientsModle.topicTitleId', this.state, this);\n            }\n        };\n        /**\n         * \n         */\n        this.showMessage = (v, name) => {\n            let page = {};\n            let code = '';\n            let inputName = '';\n            switch (v.type) {\n                case 'pageEvent': {\n                    page = { pageSize: v.pageSize, startIndex: v.pageCurrent };\n                    code = name;\n                    inputName = v.value;\n                    this.loadData(code, page, inputName);\n                    break;\n                }\n                case 'enterEvent': {\n                    this.updateInfo(name);\n                    break;\n                }\n                case 'changeEvent': {\n                    this.updateInfo(name);\n                    page = { pageSize: v.pageSize, startIndex: 1 };\n                    code = name;\n                    inputName = v.value;\n                    this.loadData(code, page, inputName);\n                    break;\n                }\n                default:\n                    page = { pageSize: 6, startIndex: 1 };\n                    code = name;\n                    inputName = v.value;\n                    this.loadData(code, page, inputName);\n                    this.loadColumns(code);\n            }\n        };\n        /**\n         * \n         */\n        this.cancel = () => {\n            this.dispatch({ visible: false });\n        };\n        /**\n         * \n         */\n        this.radioGroupChange = (name, e) => {\n            let { mrPatientsModle } = this.state;\n            mrPatientsModle[name] = e.target.value;\n            this.dispatch2({ mrPatientsModle });\n        };\n    }\n    serviceWillMount() {\n        this.reset();\n    }\n}\nexport const modifyFileService = new ModifyFileService('modifyFile');\n","source":"/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_tools_flux_BaseService__ = __webpack_require__(52);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_1_pkg_common_message__ = __webpack_require__(56);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_2_pkg_api_medical__ = __webpack_require__(81);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_2_pkg_api_medical___default = __webpack_require__.n(__WEBPACK_IMPORTED_MODULE_2_pkg_api_medical__);\n/* harmony import */ var __WEBPACK_IMPORTED_MODULE_3_tools_api_JsonUtil__ = __webpack_require__(58);\n\n\n\n// import {} from '../../../../../../packages/entity/medical'\n\nclass ModifyFileService extends __WEBPACK_IMPORTED_MODULE_0_tools_flux_BaseService__[\"a\" /* BaseService */] {\n    constructor() {\n        super(...arguments);\n        this.defaultState = {\n            dictCode: 'YSZC',\n            inputPage: {\n                startIndex: 1,\n                pageSize: 6\n            },\n            inputLength: 0,\n            dictCode1: 'YHXX',\n            inputPage1: {\n                startIndex: 1,\n         