Patient\n}\n\nclass ContentWriteService extends BaseService<ContentWriteState> {\n    defaultState = {\n        page: {pageSize: 6, startIndex: 1},\n        patientType: ''\n    }\n    // serviceWillMount(){\n    //     this.reset()\n    // }\n    /**\n     * \n     */\n    acquisition = (patient?: string, visit?: number) => {\n        let {patientType} = this.state\n        this.dispatch({patientId: patient, visitId: visit})\n        return ApiPatientMrFileIndex.GetAllMrPatientFileIndex(patient, visit, patientType).then((data: any) => {\n            if (data) {\n                for (let key in data) {\n                    if (data.hasOwnProperty(key)) {\n                        this.state[key] = data[key]\n                    }\n                }\n            }\n            this.dispatch2(this.state)\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n\n    /**\n     * \n     *\n     */\n    getPatientFileIndex = () => {\n        let {patientId, visitId, patientType, mrClassCode} = this.state\n        return ApiPatientMrFileIndex.GetPatientFileIndex(patientId, visitId, patientType, mrClassCode).then((data: any) => {\n            this.dispatch2({mrPatientFile: data})\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n    /*\n     *  \n     */\n    selectLine = (e) => {\n        return ApiPatientMrFileIndex.GetMrPatientFileHtml(e.data.id).then((data: any) => {\n            this.dispatch2({fileContent: data})\n        }).catch(err => {\n            message.tip(err || '', 'error', 'center')\n        })\n    }\n    /*\n    * \n    */\n    onchange = (name?: string, e?: any) => {\n        return new Promise((resolve, reject) => {\n            this.dispatch2({[name]: e})\n            resolve()\n        }).then(() => {\n            this.getPatientFileIndex()\n        })\n    }\n\n    /*\n     * \n     */\n    writOnchange = (e) => {\n\n        return new Promise((resolve, reject) => {\n            this.dispatch2({mrClassCode: e.key})\n            resolve()\n        }).then(() => {\n            this.getPatientFileIndex()\n        })\n    }\n}\n\nexport const contentWriteService = new ContentWriteService('contentWrite')"]}},"assets":[],"hashContent":"meta{\"harmonyModule\":true}516true[\"contentWriteService\"]52true81undefined56true11undefined11default__WEBPACK_IMPORTED_MODULE_3_debug__defaultundefinedtruetrue52BaseService__WEBPACK_IMPORTED_MODULE_0_tools_flux_BaseService__atruetrue[\"BaseService\"]81ApiPatientMrFileIndex__WEBPACK_IMPORTED_MODULE_1_pkg_api_medical__ApiPatientMrFileIndexundefinedtrue[\"ApiCruxMr\",\"ApiDataSetBdCvIndex\",\"ApiDataSetBdCvItems\",\"ApiDataSetBdDeIndex\",\"ApiDataSetBdDsIndex\",\"ApiDataSetBdFixedIndex\",\"ApiDictInput\",\"ApiDictionaryFileVisitTypeDict\",\"ApiLinkQualityPatMasterInfo\",\"ApiLinkQualityRectification\",\"ApiMonitorQcMrItems\",\"ApiMonitorQcMrVsEvent\",\"ApiPatientFileArchivalCataloging\",\"ApiPatientFileSign\",\"ApiPatientMedical\",\"ApiPatientMrFileEditApply\",\"ApiPatientMrFileIndex\",\"ApiPatientMrFilePrintLog\",\"ApiPatientMrMedicalHome\",\"ApiPatientPigeonhole\",\"ApiSystemDictParam\",\"ApiSystemLogin\",\"ApiTemplateMrClass\",\"ApiTemplateMrIndex\",\"ApiTemplateMrTempletSmallIndex\"]56message__WEBPACK_IMPORTED_MODULE_2_pkg_common_message__atruetrue[\"message\"]81ApiPatientMrFileIndex__WEBPACK_IMPORTED_MODULE_1_pkg_api_medical__ApiPatientMrFileIndexundefinedtrue[\"ApiCruxMr\",\"ApiDataSetBdCvIndex\",\"ApiDataSetBdCvItems\",\"ApiDataSetBdDeIndex\",\"ApiDataSetBdDsIndex\",\"ApiDataSetBdFixedIndex\",\"ApiDictInput\",\"ApiDictionaryFileVisitTypeDict\",\"ApiLinkQualityPatMasterInfo\",\"ApiLinkQualityRectification\",\"ApiMonitorQcMrItems\",\"ApiMonitorQcMrVsEvent\",\"ApiPatientFileArchivalCataloging\",\"ApiPatientFileSign\",\"ApiPatientMedical\",\"ApiPatientMrFileEditApply\",\"ApiPatientMrFileIndex\",\"ApiPatientMrFilePrintLog\",\"ApiPatientMrMedicalHome\",\"ApiPatientPigeonhole\",\"ApiSystemD