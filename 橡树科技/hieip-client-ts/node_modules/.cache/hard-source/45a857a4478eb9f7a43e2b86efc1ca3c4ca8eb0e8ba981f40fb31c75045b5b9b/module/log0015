l';\nclass ModifysmFileService extends BaseService {\n    constructor() {\n        super(...arguments);\n        this.defaultState = {\n            visible: false\n        };\n        /**\n         * \n         */\n        this.patient = (mrPatient, cancel) => {\n            this.dispatch2({\n                mrPatientFileEditApply: {\n                    patientId: mrPatient.patientId, visitId: mrPatient.visitId,\n                    fileUniqueId: mrPatient.id\n                },\n                fileName: mrPatient.fileName, visible: cancel\n            });\n            this.findMrTemplate();\n        };\n        /**\n         * \n         */\n        this.findMrTemplate = () => {\n            return ApiTemplateMrClass.findMrTemplate().then((data) => {\n                let me = data;\n                me.splice(0, 0, { mrClassName: '', mrClassCode: '' });\n                this.dispatch({ mrModle: data });\n            }).catch(err => {\n                message.tip(err || '', 'error', 'center');\n            });\n        };\n        /**\n         * \n         */\n        this.newMrPatientFileEditApply = () => {\n            let { mrPatientFileEditApply } = this.state;\n            return ApiPatientMrFileEditApply.NewMrPatientFileEditApply(mrPatientFileEditApply).then((data) => {\n                message.tip('', 'success', 'center');\n                this.dispatch({ visible: false, mrPatientFileEditApply: {} });\n            }).catch(err => {\n                message.tip(err || '', 'error', 'center');\n            });\n        };\n        /**\n         * \n         */\n        this.cancel = () => {\n            this.dispatch({ visible: false });\n        };\n        /**\n         * \n         */\n        this.onChange = (url, e) => {\n            const { mrPatientFileEditApply } = this.state;\n            url !== 'applyType' ? mrPatientFileEditApply[url] = e.target.value : mrPatientFileEditApply[url] = e;\n            this.dispatch2({ mrPatientFileEditApply });\n     