xtends React.Component<Aggregate, ScoreState> {\n    constructor(args) {\n        super(args)\n        this.state = {}\n        this.onHandChange = this.onHandChange.bind(this)\n    }\n\n    /** state  */\n    componentWillMount() {\n        if (this.props.other.name === 'start') {\n            const d = JSON.parse(JSON.stringify(this.props.data))\n            d.data[0].data = d.data[0].data.splice(0, 3)\n            this.setState({\n                other: this.props.other,\n                data: d,\n                active: this.props.active\n            })\n        } else {\n            this.setState({\n                other: this.props.other,\n                data: this.props.data,\n                active: this.props.active\n            })\n        }\n    }\n\n    componentWillReceiveProps(nextProps) {\n        if (nextProps.other !== this.state.other) {\n            this.setState({ other: nextProps.other })\n        }\n    }\n\n    /**\n     * @returns {any}\n     */\n    render(): JSX.Element {\n        const { data, other } = this.state\n\n        return <ScoreTrem\n            data={data.data}\n            other={other}\n            onChange={this.onHandChange}\n        />\n        // if (other.name === 'start') {\n        //     return <ScoreTrem\n        //         data={data.data}\n        //         other={other}\n        //         onChange={this.onHandChange}\n        //     />\n        // } else {\n        //     return <ScoreTrem\n        //         data={data.data}\n        //         other={other}\n        //         onChange={this.onHandChange}\n        //     />\n        // }\n    }\n\n    /**\n     * radio \n     * @param e      gcsscoreue \n     * @param type   \n     * @param term     gcs and other\n     */\n    private onHandChange(e?, type?: string, term?: string) {\n        const { value } = e.target\n        const { other } = this.state\n        const { onChange } = this.props\n        const data: any = this.state.data\n\n        let { socregcs, socreothers } = other\n\n        /**\n         * \n         * @type {number}\n         */\n\n        if (term === 'gcs') {\n            /**  gcs  */\n            socregcs[type] = value\n        } else {\n            /**    */\n            socreothers[type] = value\n        }\n\n        /**  */\n        let totalScores: number = addscore(socreothers) + (data.coreGSC ? data.coreGSC(addscore(socregcs)) : 0)\n        if (data.grade) {\n            /**   */\n\n            const resultGrade: Soceresult = data.grade(totalScores)\n            /**  */\n            other.rule = resultGrade.rule\n            /**  */\n            other.color = resultGrade.color\n        }\n\n        /**  */\n        if (data.rate) data.valuation = data.rate(totalScores)\n\n        /**  */\n        other.radioK = Object.assign({}, socreothers, socregcs)\n        /**  */\n        other.totalScores = totalScores\n\n        if (term === 'start') {\n            this.showrow(value, type)\n        } else {\n            /**  */\n            if (onChange) {\n                onChange(other)\n            }\n        }\n    }\n\n    /** start   */\n    private showrow = (value, type) => {\n        const { other } = this.state\n        const { onChange } = this.props\n        let rule: string = ''\n        let rok: any = {}\n        let T = '1__'\n        let F = '2__'\n        let newobj = JSON.parse(JSON.stringify(DT))\n\n        let newdata = newobj.data[0].data\n\n        /**  */\n        if (type === 'airway') {\n            if (1 === parseInt(value.split('__')[0], 0)) {\n                newdata = [newdata[0], newdata[1], newdata[2]]\n                rule = ' '\n                rok = {\n                    walk: F,\n                    breathing: F,\n                    airway: T\n                }\n            } else {\n                newdata = [newdata[0], newdata[1], newdata[2]]\n                rule = ' '\n                rok = {\n                    walk: F,\n                    breathing: F,\n                    airway: F\n                }\n            }\n            /**  */\n        } else if (type === 'consciousness') {\n            if (1 === parseInt(value.split('__')[0], 0)) {\n                rule = ''\n                newdata = [newdata[0], newdata[1], newdata[3], newdata[4], newdata[5]]\n                rok = {\n                    walk: F,\n                    breathing: T,\n                    brequency: F,\n                    pulse: T,\n                    consciousness: T\n                }\n            } else {\n                rule = ''\n                newdata = [newdata[0], newdata[1], newdata[3], newdata[4], newdata[5]]\n                rok = {\n                    walk: F,\n                    breathing: T,\n                    brequency: F,\n                    pulse: T,\n                    consciousness: F\n                }\n            }\n            /**  */\n        } else if (type === 'pulse') {\n            if (1 === parseInt(value.split('__')[0], 0)) {\n                rule = ''\n                newdata = [newdata[0], newdata[1], newdata[3], newdata[4], newdata[5]]\n                rok = {\n                    walk: F,\n                    breathing: T,\n                    brequency: F,\n                    pulse: T,\n                    consciousness: F\n                }\n            } else {\n                rule = ''\n                newdata = [newdata[0], newdata[1], newdata[3], newdata[4]]\n                rok = {\n                    walk: F,\n                    breathing: T,\n                    brequency: F,\n                    pulse: F\n                }\n            }\n            /**  */\n        } else if (type === 'walk') {\n            if (1 === parseInt(value.split('__')[0], 0)) {\n                newdata = [newdata[0]]\n                rule = ''\n                rok = {\n                    walk: T\n                }\n            } else {\n                newdata = [newdata[0], newdata[1], newdata[2]]\n                rule = ''\n                rok = {\n                    walk: F,\n                    breathing: F,\n                    airway: F\n                }\n            }\n            /**  */\n        } else if (type === 'breathing') {\n            if (1 === parseInt(value.split('__')[0], 0)) {\n                newdata = [newdata[0], newdata[1], newdata[3], newdata[4]]\n                rule = ''\n                rok = {\n                    walk: F,\n                    breathing: T,\n                    brequency: F,\n                    pulse: F\n                }\n            } else {\n                newdata = [newdata[0], newdata[1], newdata[2]]\n                rule = ''\n                rok = {\n                    walk: F,\n                    breathing: F,\n                    airway: F\n                }\n            }\n            /** 30/min */\n        } else if (type === 'brequency') {\n            if (1 === parseInt(value.split('__')[0], 0)) {\n                newdata = [newdata[0], newdata[1], newdata[3]]\n                rule = ''\n                rok = {\n                    walk: F,\n                    breathing: T,\n                    brequency: T\n                }\n            } else {\n                newdata = [newdata[0], newdata[1], newdata[3], newdata[4]]\n                rule = ''\n                rok = {\n                    walk: F,\n                    breathing: T,\n                    brequency: F,\n                    pulse: F\n                }\n            }\n        }\n        newobj.data[0].data = newdata\n        other.rule = rule\n        other.radioK = rok\n        this.setState({ data: newobj, other })\n        if (onChange) {\n            onChange(other)\n        }\n    }\n}\n\n/**\n *  \n * @param obj\n * @returns {number}\n */\nfunction addscore(obj): number {\n    let sorce: number = 0\n    for (const prop in obj) {\n        if (prop) {\n            let key = obj[prop].split('__')\n            sorce += Number.parseInt(key[0], 0)\n        }\n    }\n    return sorce\n}\n"]},"baseMapRemovedQuality":"","baseMap":{"version":3,"sources":["/Users/wujunlong/Documents/work/hieip-client-ts/src/packages/triage-score/socre/score.tsx"],"names":[],"mappings":";;AAAA;AAAA;;;;;;;GAOG;AAES;AAEkB;AACS;AAGvC,IAAI,EAAE,GAAQ;IACV,IAAI,EAAE;QACF;YACI,IAAI,EAAE,EAAE;YACR,IAAI,EAAE,OAAO;YACb,IAAI,EAAE;gBACF;oBACI,IAAI,EAAE,QAAQ;oBACd,IAAI,EAAE,MAAM;oBACZ,IAAI,EAAE;wBACF,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;wBACvB,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;qBAC1B;iBACJ,EAAE;oBACC,IAAI,EAAE,QAAQ;oBACd,IAAI,EAAE,WAAW;oBACjB,IAAI,EAAE;wBACF,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;wBACvB,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;qBAC1B;iBACJ,EAAE;oBACC,IAAI,EAAE,aAAa;oBACnB,IAAI,EAAE,QAAQ;oBACd,IAAI,EAAE;wBACF,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;wBACvB,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;qBAC1B;iBACJ,EAAE;oBACC,IAAI,EAAE,eAAe;oBACrB,IAAI,EAAE,WAAW;oBACjB,IAAI,EAAE;wBACF,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;wBACvB,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;qBAC1B;iBACJ,EAAE;oBACC,IAAI,EAAE,QAAQ;oBACd,IAAI,EAAE,OAAO;oBACb,IAAI,EAAE;wBACF,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;wBACvB,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;qBAC1B;iBACJ,EAAE;oBACC,IAAI,EAAE,mBAAmB;oBACzB,IAAI,EAAE,eAAe;oBACrB,IAAI,EAAE;wBACF,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;wBACvB,EAAE,IAAI,EAAE,GAAG,EAAE,KAAK,EAAE,CAAC,EAAE;qBAC1B;iBACJ;aACJ;SACJ;KACJ;CACJ;AASD,cAAc;AACR,WAAa,SAAQ,gDAAsC;IAC7D,YAAY,IAAI;QACZ,KAAK,CAAC,IAAI,CAAC;QAiHf,gBAAgB;QACR,YAAO,GAAG,CAAC,KAAK,EAAE,IAAI,EAAE,EAAE;YAC9B,MAAM,EAAE,KAAK,EAAE,GAAG,IAAI,CAAC,KAAK;YAC5B,MAAM,EAAE,QAAQ,EAAE,GAAG,IAAI,CAAC,KAAK;YAC/B,IAAI,IAAI,GAAW,EAAE;YACrB,IAAI,GAAG,GAAQ,EAAE;YACjB,IAAI,CAAC,GAAG,MAAM;YACd,IAAI,CAAC,GAAG,MAAM;YACd,IAAI,MAAM,GAAG,IAAI,CAAC,KAAK,CAAC,IAAI,CAAC,SAAS,CAAC,EAAE,CAAC,CAAC;YAE3C,IAAI,OAAO,GAAG,MAAM,CAAC,IAAI,CAAC,CAAC,CAAC,CAAC,IAAI;YAEjC,kBAAkB;YAClB,EAAE,CAAC,CAAC,IAAI,KAAK,QAAQ,CAAC,CAAC,CA