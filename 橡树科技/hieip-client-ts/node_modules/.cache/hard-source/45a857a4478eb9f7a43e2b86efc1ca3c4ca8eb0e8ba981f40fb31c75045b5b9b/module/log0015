      page = { pageSize: v.pageSize, startIndex: v.pageCurrent };\n                    code = name;\n                    inputName = v.value;\n                    this.loadData(code, page, inputName);\n                    break;\n                }\n                case 'enterEvent': {\n                    this.updateInfo(name);\n                    break;\n                }\n                case 'changeEvent': {\n                    this.updateInfo(name);\n                    page = { pageSize: v.pageSize, startIndex: 1 };\n                    code = name;\n                    inputName = v.value;\n                    this.loadData(code, page, inputName);\n                    break;\n                }\n                default:\n                    page = { pageSize: 6, startIndex: 1 };\n                    code = name;\n                    inputName = v.value;\n                    this.loadData(code, page, inputName);\n                    this.loadColumns(code);\n            }\n        };\n        /**\n         * \n         */\n        this.cancel = () => {\n            this.dispatch({ visible: false });\n        };\n        /**\n         * \n         */\n        this.radioGroupChange = (name, e) => {\n            let { mrPatientsModle } = this.state;\n            mrPatientsModle[name] = e.target.value;\n            this.dispatch2({ mrPatientsModle });\n        };\n    }\n    serviceWillMount() {\n        this.reset();\n    }\n}\nconst modifyFileService = new ModifyFileService('modifyFile');\n/* harmony export (immutable) */ __webpack_exports__[\"a\"] = modifyFileService;\n\n","sourceMap":{"mapRemovedQuality":"","map":{"version":3,"sources":["/Users/wujunlong/Documents/work/hieip-client-ts/src/service/medical/medical/components/written/modify-file/index.ts"],"names":[],"mappings":";;;;;AAAkD;AAGR;AAEhB;AAC1B,6DAA6D;AAClB;AAuC3C,uBAAwB,SAAQ,2EAA4B;IAA5D;;QACI,iBAAY,GAAG;YACX,QAAQ,EAAC,MAAM;YACf,SAAS,EAAE;gBACP,UAAU,EAAE,CAAC;gBACb,QAAQ,EAAE,CAAC;aACd;YACD,WAAW,EAAC,C