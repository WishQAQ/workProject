  "description": "橡树电子病历系统",
  "scripts": {
    "start": "electron -r babel-register ./server --port 6101 --dev_port 6103 --dev",
    "start:webpack": "webpack-dev-server --config scripts/webpack.config.babel.js --port 6103 --inline --hot",
    "build:dist": "gulp --gulpfile=scripts/gulpfile.babel.js build --cwd .",
    "build:ddl:production": "gulp --gulpfile=scripts/gulpfile.babel.js build:ddl -m --cover --cwd .",
    "build:ddl:development": "gulp --gulpfile=scripts/gulpfile.babel.js build:ddl --cover --cwd .",
    "update:minor": "gulp --gulpfile=scripts/gulpfile.babel.js build:package -u --cwd .",
    "update:primary": "gulp --gulpfile=scripts/gulpfile.babel.js build:package -U --cwd .",
    "wepack:stats": "webpack --config scripts/webpack.config.babel.js --output-path='./dist/build/assets' --profile --json > ./stats.json",
    "wepack:ddl:stats": "cd scripts && webpack --config webpack.dll.config.babel.js --output-path='../dist/ddl' --profile --json > ../stats.ddl.json",
    "build:publish": "gulp --gulpfile=scripts/gulpfile.babel.js build:client --cwd .",
    "publish": "npm run build:publish && cd dist/client && npm publish && cd ../word && npm publish",
    "test": "babel-node test/index.jsx"
  },
